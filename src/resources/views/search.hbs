<form method='get'>
    <div class='mb-3'>
        <label for='search-input' class='form-label'>Tìm kiếm</label>
        <input
            type='text'
            name='q'
            class='form-control'
            id='search-input'
            placeholder='Nhập tìm kiếm...'
        />
    </div>

    <button type='submit' class='btn btn-primary'>Search</button>
</form>

{{! 
    query param: (trên url): req.query
    form data: req.body
 }}